#--------------------------- Tool Specifications -------------------------
#
# Modify the following macros to reflect the tools you wish to use for
# compiling and linking your code.
#
CC = /usr/local/MATLAB/R2017b/bin/mex 
LD = $(CC)
 
MODEL     = DetectAndTrackFace
TARGET      = sfun
MODULE_SRCS   = c1_DetectAndTrackFace.c c2_DetectAndTrackFace.c
MODEL_SRC  = DetectAndTrackFace_sfun.c
MODEL_REG = DetectAndTrackFace_sfun_registry.c
MAKEFILE    = DetectAndTrackFace_sfun.mku
MATLAB_ROOT  = /usr/local/MATLAB/R2017b/toolbox/stateflow/stateflow/../../..
BUILDARGS   = 
#------------------------------ Include/Lib Path ------------------------------
 
USER_INCLUDES = 
AUX_INCLUDES = 
MLSLSF_INCLUDES  = \
    -I"/usr/local/MATLAB/R2017b/extern/include" \
    -I"/usr/local/MATLAB/R2017b/simulink/include" \
    -I"/usr/local/MATLAB/R2017b/simulink/include/sf_runtime" \
    -I"/usr/local/MATLAB/R2017b/stateflow/c/mex/include" \
    -I"/usr/local/MATLAB/R2017b/rtw/c/src" \
    -I"/home/perkjelsvik/MATLAB-Drive/CompVis/Computer-Vision-Utwente/code/test/slprj/_sfprj/DetectAndTrackFace/_self/sfun/src" 

THIRD_PARTY_INCLUDES = -I"/usr/local/MATLAB/R2017b/toolbox/vision/builtins/src/ocv/include" -I"/usr/local/MATLAB/R2017b/toolbox/vision/builtins/src/ocvcg/opencv/include" 

INCLUDE_PATH = $(USER_INCLUDES) $(AUX_INCLUDES) $(MLSLSF_INCLUDES) $(COMPILER_INCLUDES) $(THIRD_PARTY_INCLUDES)
 
#----------------- Compiler and Linker Options --------------------------------
 
# Optimization Options
 
CC_OPTS = 
CPP_REQ_DEFINES = -DMATLAB_MEX_FILE
 
# Uncomment this line to move warning level to W4
# cflags = $(cflags:W3=W4)
CFLAGS = $(CC_OPTS) $(CPP_REQ_DEFINES) $(INCLUDE_PATH)
 
LDFLAGS =  
 
AUXLDFLAGS = 
#----------------------------- Source Files -----------------------------------
 
REQ_SRCS  = $(MODEL_SRC) $(MODEL_REG) $(MODULE_SRCS)

USER_ABS_OBJS =

AUX_ABS_OBJS =
THIRD_PARTY_OBJS     = \
		CascadeClassifierCore.o \
		mwcascadedetect.o \
		mwhaar.o \
		cgCommon.o \
		pointTrackerCore.o \

REQ_OBJS = $(REQ_SRCS:.cpp=.o)
REQ_OBJS2 = $(REQ_OBJS:.c=.o)
OBJS = $(REQ_OBJS2) $(USER_ABS_OBJS) $(AUX_ABS_OBJS) $(THIRD_PARTY_OBJS)
OBJLIST_FILE = DetectAndTrackFace_sfun.mol
SFCLIB = 
AUX_LNK_OBJS = 
USER_LIBS = 
PARLIB = 
  MAPCSF = /usr/local/MATLAB/R2017b/tools/glnxa64/mapcsf
   # RUN_MAPCSF_ON_UNIX is defined only if MAPCSF exists on this platform.
   ifneq ($(wildcard $(MAPCSF)),) # run MAPCSF if it exists on this platform
      RUN_MAPCSF_ON_UNIX =  /usr/local/MATLAB/R2017b/tools/glnxa64/mapcsf $@
   endif
 
#--------------------------------- Rules --------------------------------------
 
MEX_FILE_NAME = $(MODEL)_$(TARGET).mexa64
 
 $(MEX_FILE_NAME): $(MAKEFILE) $(OBJS) $(SFCLIB) $(AUX_LNK_OBJS) $(USER_LIBS)
	@echo ### Linking ...
	$(CC) -silent LDFLAGS="\$$LDFLAGS $(LDFLAGS) $(AUXLDFLAGS)" -output $(MEX_FILE_NAME) @$(OBJLIST_FILE) $(AUX_LNK_OBJS) $(USER_LIBS) $(SFCLIB) $(PARLIB) $(IPPLIB) $(THIRD_PARTY_LIBS)
	$(RUN_MAPCSF_ON_UNIX)

%.o :    %.c
	$(CC) -c $(CFLAGS) $<

%.o :    %.cpp
	$(CC) -c $(CFLAGS) $<

CascadeClassifierCore.o :	/usr/local/MATLAB/R2017b/toolbox/vision/builtins/src/ocv/CascadeClassifierCore.cpp
	$(CC) -c $(CFLAGS) /usr/local/MATLAB/R2017b/toolbox/vision/builtins/src/ocv/CascadeClassifierCore.cpp
mwcascadedetect.o :	/usr/local/MATLAB/R2017b/toolbox/vision/builtins/src/ocv/mwcascadedetect.cpp
	$(CC) -c $(CFLAGS) /usr/local/MATLAB/R2017b/toolbox/vision/builtins/src/ocv/mwcascadedetect.cpp
mwhaar.o :	/usr/local/MATLAB/R2017b/toolbox/vision/builtins/src/ocv/mwhaar.cpp
	$(CC) -c $(CFLAGS) /usr/local/MATLAB/R2017b/toolbox/vision/builtins/src/ocv/mwhaar.cpp
cgCommon.o :	/usr/local/MATLAB/R2017b/toolbox/vision/builtins/src/ocv/cgCommon.cpp
	$(CC) -c $(CFLAGS) /usr/local/MATLAB/R2017b/toolbox/vision/builtins/src/ocv/cgCommon.cpp
pointTrackerCore.o :	/usr/local/MATLAB/R2017b/toolbox/vision/builtins/src/ocv/pointTrackerCore.cpp
	$(CC) -c $(CFLAGS) /usr/local/MATLAB/R2017b/toolbox/vision/builtins/src/ocv/pointTrackerCore.cpp
